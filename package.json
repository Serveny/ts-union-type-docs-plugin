{
    "name": "ts-union-type-docs-plugin",
    "version": "0.1.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "prepublishOnly": "npm run build",
    "repository": {
        "type": "git",
        "url": "https://github.com/Serveny/ts-union-type-docs-plugin"
    },
    "devDependencies": {
        "@types/node": "24.9.2",
        "rollup-plugin-node-externals": "8.1.1",
        "typescript": "5.9.3",
        "vite": "7.1.12",
        "vite-plugin-dts": "4.5.4"
    },
    "scripts": {
        "check": "tsc --noEmit",
        "build": "vite build",
        "watch": "vite",
        "refresh": "npm run build && cd ../example && npm i",
        "refresh-watch": "npx nodemon -w ./src -e ts -x \"npm run build && cd ../example && npm i\"",
        "debug": "TSS_DEBUG=5667 code ../example --user-data-dir ~/.vscode-debug/"
    }
}